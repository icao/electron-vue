	"tokenColors": [
		{
			"name": "Changed",
			"scope": [
				"markup.changed",
				"meta.diff.header.git",
				"meta.diff.header.from-file",
				"meta.diff.header.to-file"
			],
			"settings": {
				"foreground": "#a2bffc",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted.diff",
			"settings": {
				"foreground": "#ff587490",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted.diff",
			"settings": {
				"foreground": "#6af699ff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#506686",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#6af699"
			}
		},
		{
			"name": "String Quoted",
			"scope": [
				"string.quoted",
				"variable.other.readwrite.js"
			],
			"settings": {
				"foreground": "#6af699"
			}
		},
		{
			"name": "Support Constant Math",
			"scope": "support.constant.math",
			"settings": {
				"foreground": "#ffbf7a"
			}
		},
